{
    "openapi": "3.0.0",
    "info": {
        "title": "TinderKW API",
        "description": "Tinder-like API for mobile applications with device-based JWT authentication",
        "contact": {
            "name": "TinderKW Support",
            "email": "admin@tinderkw.com"
        },
